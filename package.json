{
  "name": "webpack-msbuild-plugin",
  "version": "2.0.0",
  "description": "A webpack plugin to handle the automation of running msbuild scripts. It uses vswhere to locate the appropriate msbuild. Microsoft is shipping vswhere from Visual Studio 2017",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src --out-dir lib",
    "watch": "babel src --watch --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azadar/webpack-msbuild-plugin.git"
  },
  "author": "Shannon hochkins",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/azadar/webpack-msbuild-plugin/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/azadar/webpack-msbuild-plugin#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1"
  },
  "dependencies": {
    "deep-extend": "^0.5.0"
  }
}
